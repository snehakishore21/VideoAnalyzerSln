@page "/Keywords"
<ul class="SG">
   <li class="sgLi">
    <div class="box">
    <h3>Keywords</h3>
    @if (this.KeywordsInfoResult != null)
    {
        <ul >
            @foreach (var singleKeyword in this.KeywordsInfoResult)
            {
            <li >
                <span class="badge badge-primary badge-pill">@singleKeyword.Appeareances</span>
                <a href="Search/@singleKeyword.Keyword">@singleKeyword.Keyword</a>
            </li>
            }
        </ul>
    }
     </div>
 </li> 
